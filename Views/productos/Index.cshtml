@{
    ViewData["Title"] = "Productos";

}

<h1>Productos</h1>

<table class="table">
    <thead>
        <tr>
            <th>Id</th>
            <th>Nombre</th>
            <th>Precio</th>
        </tr>
    </thead>
    <tbody class="table-group-divider">
        @foreach (var producto in Model)
        {
            <tr>
                <td>@producto.Id</td>
                <td>@producto.Nombre</td>
                <td>Q. @producto.Precio</td>
            </tr>
        }
    </tbody>
</table>